{
  "name": "yek",
  "version": "3.0.0",
  "description": "Generates a key path from arrays and back again.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint:fix": "eslint --fix \"src/**/*.ts\" \"tests/**/*.ts\"",
    "check": "npm run typecheck && npm run lint && npm test",
    "prepublishOnly": "npm run check && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kahwee/yek.git"
  },
  "keywords": [
    "keypath",
    "notation",
    "bracket",
    "array"
  ],
  "author": "KahWee Teng <t@kw.sg> (http://kw.sg/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kahwee/yek/issues"
  },
  "homepage": "https://github.com/kahwee/yek",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.17.0",
    "globals": "^16.5.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.46.3",
    "vitest": "^4.0.16"
  }
}
