{
  "name": "yek",
  "version": "3.0.0",
  "description": "Generates a key path from arrays and back again.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint:fix": "eslint --fix \"src/**/*.ts\" \"tests/**/*.ts\"",
    "check": "npm run typecheck && npm run lint && npm test",
    "prepublishOnly": "npm run check && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kahwee/yek.git"
  },
  "keywords": [
    "keypath",
    "notation",
    "bracket",
    "array"
  ],
  "standard": {
    "ignore": [
      "/dist",
      "/coverage",
      "/reports"
    ]
  },
  "author": "KahWee Teng <t@kw.sg> (http://kw.sg/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kahwee/yek/issues"
  },
  "homepage": "https://github.com/kahwee/yek",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.3.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3", 
    "vitest": "^1.3.0"
  }
}
